use fmt for std::fmt
use std::math::rand::{Rand}
use time for std::time::{Time, Duration}

fn main() {
    let mut rand = Rand.New(Time.Now().Unix())
    let mut i = 0
    for i < 10; i++ {
        let dur = Duration.Millisecond * rand.Nextn63(1000)
        //fmt::Printf("Sleeping for {}ms\n", Duration.Milliseconds(dur))
        outln(dur)
        time::Sleep(dur)
    }
    fmt::Println("Done!")
}